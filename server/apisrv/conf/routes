# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET		/manga/seeder/index_old		@com.mangafun.controllers.MangaSeederController.index_old()
GET		/manga/seeder/index			@com.mangafun.controllers.MangaSeederController.index(mangaIndex: Int ?= 0)
GET		/manga/seeder/getfromto		@com.mangafun.controllers.MangaSeederController.getfromto(startingIndex: Int, endingIndex: Int)
GET		/manga/seeder/image			@com.mangafun.controllers.MangaSeederController.image(mangaIndex: Int ?= 0)
GET		/manga/seeder/imagefromto	@com.mangafun.controllers.MangaSeederController.imagefromto(startingIndex: Int, endingIndex: Int)

GET		/manga/api/rnfetchget		@com.mangafun.controllers.ReactNativeFetchController.fetchget()
POST	/manga/api/rnfetchpost		@com.mangafun.controllers.ReactNativeFetchController.fetchpost()
POST	/manga/api/signin/createuser		@com.mangafun.controllers.SignInController.createuser()
POST	/manga/api/signin/signinuser		@com.mangafun.controllers.SignInController.signinuser()
POST	/manga/api/signin/signinsession		@com.mangafun.controllers.SignInController.signinsession()

GET		/aonhub/seeder/updatemangajson		@com.mangafun.controllers.AonhubSeederController.updatemangajson()
GET		/aonhub/seeder/getmangainfo			@com.mangafun.controllers.AonhubSeederController.getmangainfo(mangaId: Int ?= 2940)
GET		/aonhub/seeder/getfromto			@com.mangafun.controllers.AonhubSeederController.getfromto(startingIndex: Int, endingIndex: Int)
GET		/aonhub/seeder/image				@com.mangafun.controllers.AonhubSeederController.image(mangaId: Int ?= 2940)
GET		/aonhub/seeder/imagefromto			@com.mangafun.controllers.AonhubSeederController.imagefromto(startingIndex: Int, endingIndex: Int)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               @controllers.Assets.at(path="/public", file)